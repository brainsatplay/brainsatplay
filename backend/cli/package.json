{
  "name": "brainsatplay-cli",
  "version": "0.0.0",
  "description": "The Universal Web Development CLI",
  "main": "src/index.js",  
  "type": "module",
  
  "bin": {
    "brainsatplay": "./src/index.js"
  },
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "start": "tinybuild",
    "docs": "cd docs && yarn && yarn run start",
    "backend": "node builder.config.js run:nodejs",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest --runInBand --coverage",
    "typecheck": "tsc --watch --preserveWatchOutput --noEmit",
    "build": "cd docs && yarn && yarn build",
    "killall-node": "killall node",
    "killall-node:windows": "taskkill /F /IM node.exe",
    "cli": "npm i -g ./src/cli",
    "tinybuild": "npm i -g ./src/build",
    "ui": "cd src/visualscript && yarn start",


    "watch": "node src/watch.js"

  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/brainsatplay/brainsatplay.git"
  },
  "author": "Garrett Flynn <garrettmflynn@gmail.com>",
  "license": "AGPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/brainsatplay/brainsatplay/issues"
  },
  "homepage": "https://github.com/brainsatplay/brainsatplay#readme",
  "keywords": [
    "cli"
  ],

  "dependencies": {
    "chalk": "^5.0.0",
    "commander": "^8.3.0",
    "conf": "^10.1.1",
    "cross-spawn": "^7.0.3",
    "inquirer": "^8.2.0",
    "simple-git": "^2.48.0",
    "node-html-parser": "^6.1.1"
  },

  "devDependencies": {
    "@babel/core": "^7.16.7",
    "@babel/plugin-transform-runtime": "^7.17.0",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.1",
    "@types/jest-environment-puppeteer": "^5.0.0",
    "@types/node": "^17.0.10",
    "@types/puppeteer": "^5.4.5",
    "babel-jest": "^27.5.1",
    "babel-loader": "^8.2.3",
    "build-dev": "^0.0.10",
    "concurrently": "^7.0.0",
    "cors": "^2.8.5",
    "datastreams-api": "0.0.14",
    "dotenv": "^14.2.0",
    "express": "^4.17.2",
    "jest": "^27.5.1",
    "jest-puppeteer": "^6.1.0",
    "mongoose": "^6.1.6",
    "ts-jest": "^27.1.4",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.7.0",
    "tslib": "^2.3.1"
  }
}
